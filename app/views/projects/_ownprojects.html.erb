<ul class="media-list">
  <% @projects.each do |project| %>
    <li class="media">
      <div class="media-body">
        <div>
          <%= link_to project.user.name, user_path(project.user) %> <span class="text-muted">posted at <%= project.created_at %></span>
        </div>
        <div>
          <%= link_to project.title, project_path(project) %>
        </div>
      </div>
    </li>
  <% end %>
  <%= paginate @projects %>
</ul>